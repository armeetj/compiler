start:
    callq read_int
    movq %rax, %rbx
    callq read_int
    movq %rax, -16(%rbp)
    callq read_int
    movq %rax, %rcx
    addq -16(%rbp), %rbx
    movq %rbx, %rax
    addq %rcx, %rax
    jmp conclusion

    .globl main

main:
    pushq %rbp
    movq %rsp, %rbp
    pushq %rbx
    subq $8, %rsp
    jmp start

conclusion:
    addq $8, %rsp
    popq %rbx
    popq %rbp
    retq
