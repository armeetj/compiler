block_1:
    movq free_ptr(%rip), %r11
    addq $24, free_ptr(%rip)
    movq $5, 0(%r11)
    movq %r11, -8(%r15)
    movq -8(%r15), %r11
    movq $40, 8(%r11)
    movq $0, %rbx
    movq -8(%r15), %r11
    movq $2, 16(%r11)
    movq $0, %rbx
    movq -8(%r15), %rbx
    movq %rbx, %r11
    movq 8(%r11), %rax
    movq %rax, -8(%r15)
    movq %rbx, %r11
    movq 16(%r11), %rbx
    movq -8(%r15), %rax
    addq %rbx, %rax
    jmp conclusion

block_2:
    movq $0, %rbx
    jmp block_1

block_3:
    movq %r15, %rdi
    movq $24, %rsi
    callq collect
    jmp block_1

start:
    movq free_ptr(%rip), %rbx
    addq $24, %rbx
    movq fromspace_end(%rip), %rax
    movq %rax, -16(%rbp)
    cmpq -16(%rbp), %rbx
    jl block_2
    jmp block_3

    .globl main

main:
    pushq %rbp
    movq %rsp, %rbp
    pushq %rbx
    subq $8, %rsp
    movq $65536, %rdi
    movq $65536, %rsi
    callq initialize
    movq rootstack_begin(%rip), %r15
    movq $0, 0(%r15)
    addq $8, %r15
    jmp start

conclusion:
    subq $8, %r15
    addq $8, %rsp
    popq %rbx
    popq %rbp
    retq
