block_1:
    movq free_ptr(%rip), %r11
    addq $16, free_ptr(%rip)
    movq $3, 0(%r11)
    movq %r11, -8(%r15)
    movq -8(%r15), %r11
    movq %rbx, 8(%r11)
    movq $0, %rcx
    movq -8(%r15), %rcx
    movq %rcx, %r11
    movq 8(%r11), %rcx
    movq %rcx, %rax
    addq $1, %rax
    jmp conclusion

block_2:
    movq $0, %rcx
    jmp block_1

block_3:
    movq %r15, %rdi
    movq $16, %rsi
    callq collect
    jmp block_1

block_4:
    movq free_ptr(%rip), %rcx
    addq $16, %rcx
    movq fromspace_end(%rip), %rax
    movq %rax, -16(%rbp)
    cmpq -16(%rbp), %rcx
    jl block_2
    jmp block_3

block_5:
    movq $41, %rbx
    jmp block_4

block_6:
    movq $47, %rbx
    jmp block_4

start:
    callq read_int
    movq %rax, %rcx
    cmpq $1, %rcx
    je block_5
    jmp block_6

    .globl main

main:
    pushq %rbp
    movq %rsp, %rbp
    pushq %rbx
    subq $8, %rsp
    movq $65536, %rdi
    movq $65536, %rsi
    callq initialize
    movq rootstack_begin(%rip), %r15
    movq $0, 0(%r15)
    addq $8, %r15
    jmp start

conclusion:
    subq $8, %r15
    addq $8, %rsp
    popq %rbx
    popq %rbp
    retq
