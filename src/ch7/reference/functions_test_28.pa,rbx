main_start:
    leaq id(%rip), %rbx
    movq $0, %rdi
    movq %rbx, %rax
    addq $8, %rsp
    popq %rbx
    popq %rbp
    jmp *%rax

    .globl main
    .align 8

main:
    pushq %rbp
    movq %rsp, %rbp
    pushq %rbx
    subq $8, %rsp
    movq $65536, %rdi
    movq $65536, %rsi
    callq initialize
    movq rootstack_begin(%rip), %r15
    jmp main_start

main_conclusion:
    addq $8, %rsp
    popq %rbx
    popq %rbp
    retq

block_1:
    movq $0, %rax
    jmp id_conclusion

block_2:
    movq $42, %rax
    jmp id_conclusion

id_start:
    movq %rdi, %rbx
    cmpq $0, %rbx
    je block_1
    jmp block_2

    .globl id
    .align 8

id:
    pushq %rbp
    movq %rsp, %rbp
    pushq %rbx
    subq $8, %rsp
    jmp id_start

id_conclusion:
    addq $8, %rsp
    popq %rbx
    popq %rbp
    retq
